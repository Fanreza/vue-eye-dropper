{
  "name": "vue-eye-dropper",
  "version": "0.0.4",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "src/components/"
  ],
  "main": "./dist/vue-eye-dropper.umd.js",
  "module": "./dist/vue-eye-dropper.js",
  "exports": {
    ".": {
      "import": "./dist/vue-eye-dropper.js",
      "require": "./dist/vue-eye-dropper.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./dist/index.d.ts": {
      "import": "./dist/index.d.ts",
      "require": "./dist/index.d.ts"
    },
    "./dist/components/EyeDropper.vue.d.ts": {
      "import": "./dist/components/EyeDropper.vue.d.ts",
      "require": "./dist/components/EyeDropper.vue.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fanreza/vue-eye-dropper.git"
  },
  "keywords": [
    "eyedropper",
    "vue3",
    "vue",
    "vuejs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "path": "^0.12.7",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.3",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.4.15"
  }
}
