{
  "name": "vue-eye-dropper",
  "version": "0.0.9",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-eye-dropper.umd.js",
  "module": "./dist/vue-eye-dropper.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-eye-dropper.es.js",
      "require": "./dist/vue-eye-dropper.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fanreza/vue-eye-dropper.git"
  },
  "keywords": [
    "eyedropper",
    "vue3",
    "vue",
    "vuejs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "exit 0"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "path": "^0.12.7",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vite-plugin-css-injected-by-js": "^3.4.0",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.4.15"
  }
}
